<div class="bar-loading">
  <% sum = 0 %>
    <% Loadingmaterial.where("loading_id <= #{loading.id}").and(Loadingmaterial.where(loading: Loading.where(delivery_id: loading.delivery))).each do |lm| %>
    <% sum = sum + lm.quantity %>
    <% end %>
  <div class="progress">
    <div class="progress-bar " role="progressbar" style="width: <%= (sum.to_f/33*100).round %>%;" aria-valuenow="<%= sum %>" aria-valuemin="0" aria-valuemax="33"></div>
  </div>
  <div id="icon">
  <%= link_to delivery_loading_path(@delivery, loading) do %>
    <%= icon('fa-solid', 'truck') %>
  <% end %>
  </div>
</div>
