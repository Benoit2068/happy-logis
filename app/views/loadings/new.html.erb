<%= simple_form_for [@delivery, @loading] do |f| %>
  <%= f.label :name %>
  <%= f.input :name , as: :string %>
  <%= f.label :address %>
  <%= f.input :address , as: :string %>
  <%= f.label :date %>
  <%= f.input :date , label: '(YYYY/MM/DD)'%>
  <%= f.submit "Create Loading", class: "btn btn-primary" %>
<% end %>
